                              data-widths="[180, 360, 540, 720]"
                              data-sizes="auto">
                        {{/if}}
                      </div>
                    </div>
                  </div>

                  <div class="grid-product__meta">
                    {{title}}
                  </div>
                </a>
              </div>
            {{/articles}}
          </div>
        </div>
      {{/if}}
    
  </div>
</script>
